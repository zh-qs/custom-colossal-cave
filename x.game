      description:
        - true:
            THE DOME IS UNCLIMBABLE.
      onEntry:
        {
          goto room15
        }
      items:
      entities:
      commands:
  - room23:
      description:
        - true:
            YOU ARE AT THE WEST END OF THE TWOPIT ROOM.  THERE IS A LARGE HOLE IN
