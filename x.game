              if true then
              {
              goto room32
              }
              else
              {
              call SECRE
              }
            }
        - SECRE:
            {
              goto room74
            }
  - room20:
      description:
        - true:
            YOU ARE AT THE BOTTOM OF THE PIT WITH A BROKEN NECK.
      onEntry:
        {
          goto room0
        }
      items:
      entities:
      commands:
  - room21:
      description:
        - true:
            YOU DIDN'T MAKE IT.
      onEntry:
        {
          goto room0
        }
      items:
      entities:
      commands:
